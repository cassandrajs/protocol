{
	"printWidth": 80,
	"singleQuote": false,
	"semi": false,
	"useTabs": true,
	"trailingComma": "es5",
	"plugins": ["@ianvs/prettier-plugin-sort-imports"],
	"importOrder": [
		"<BUILTIN_MODULES>",
		"<THIRD_PARTY_MODULES>",
		"^[.]{2}$",
		"^[.]{2}/",
		"^[.]/(?!index)",
		"^[.]$",
		"^[.]/index$",
		"<TYPES>"
	],
	"importOrderTypeScriptVersion": "4.8.4"
}
